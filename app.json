{
  "expo": {
    "name": "Workout Wave",
    "slug": "workout-wave",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "scheme": "workoutwave",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.workoutwave.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1011295206743-rl70k9ibahkkgkf41j8qr6vfneedgb8s"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.workoutwave.app",
      "permissions": [
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-web-browser",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "color": "#4CAF50"
        }
      ]
    ],
    "notification": {
      "color": "#4CAF50",
      "androidMode": "default",
      "androidCollapsedTitle": "Rest Timer"
    }
  }
}
